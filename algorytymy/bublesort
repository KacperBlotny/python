def sortowanie(lista):
    n = len(lista)
    for i in range(n):
        for x in range(0, n-i-1):
            if lista[x] > lista[x+1]:
                lista[x], lista[x+1] = lista[x+1], lista[x]
    


lista = [5,4,3,2,1,5,6,4,3,2,5,6,7,2]
sortowanie(lista)

print ("Sorted array is:")
for i in range(len(lista)):
    print ("%d" %lista[i])
